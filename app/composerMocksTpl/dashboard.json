{
  "template": {
    "tag": "cells-template-paper-drawer-panel",
    "familyPath": "@cells-components/cells-template-paper-drawer-panel",
    "render": "litElement",
    "properties": {
      "mode": "seamed",
      "zones": [
        "app__header",
        "app__main",
        "app__footer"
      ]
    }
  },
  "components": [
    {
      "zone": "app__header",
      "type": "UI",
      "familyPath": "@bbva-web-components/bbva-header-main",
      "tag": "bbva-header-main",
      "render": "litElement",
      "properties": {
        "icon-left-primary": "bbva:return",
        "text": "Poke list"
      }
    },
    {
      "zone": "app__main",
      "type": "DM",
      "familyPath": "../pokemons-dm",
      "tag": "pokemons-dm",
      "render": "litElement",
      "properties": {
        "cellsConnections": {
          "in": {
            "list-pagination": {
              "bind": "getPage"
            }
          },
          "out": {
            "result-list": {
              "bind": "firstData-event"
            }
          }
        }
      }
    }
  ]
}
